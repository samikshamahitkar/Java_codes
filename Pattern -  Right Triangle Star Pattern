/*
Output

*
**
***
****
*****

*/


class patternA{
   
   void printSquare(int n) {
        
        for(int i=0;i<n;i++){
            for(int j=0;j<=i;j++){
                System.out.print("*");
            }
            System.out.print("\n");
           
        }
    } 

   
  public static void main(String args[]) {
     int n =5;
    patternA m3=new patternA();
    m3.printSquare(n);
    
    
    
  }
}
