class Count{
    int countdigit(int n){
        int count=0;
        while(n>0){
            n=n/10;
            count++;
        }
        return count;
    }
    public static void main (String[] args) {
        int n=1;
        Count c = new Count();
        int a=c.countdigit( n);
        System.out.println(a);
    }
}
