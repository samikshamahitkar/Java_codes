class ReverseStr{

void Reverse(String s) {
        char c[]=s.toCharArray();
         String a=" ";
    
         for(int i=c.length-1;i>=0;i--){
            a=a+c[i];
            
        }
        
        System.out.println(a);
        
        } 

     public static void main(String args[]) {
     String s="My name is lara";
     ReverseStr m3=new ReverseStr();
     m3.Reverse(s);
      
}
}
