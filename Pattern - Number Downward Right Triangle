/*

Output
11111
2222
333
44
5

*/


class patternA{
   
   void printSquare(int n) {
        int count=1;
        for(int i=n;i>=0;i--){
            for(int j=1;j<=i;j++){
                System.out.print(count);
                
            }
            System.out.print("\n");
            count++;
           
        }
    } 

   
  public static void main(String args[]) {
     int n =5;
    patternA m3=new patternA();
    m3.printSquare(n);
    
    
    
  }
}
