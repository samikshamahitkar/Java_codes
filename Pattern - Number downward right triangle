/*
Output
12345
1234
123
12
1
*/


class patternA{
   
   void printSquare(int n) {
        
        for(int i=n;i>=0;i--){
            for(int j=1;j<=i;j++){
                System.out.print(j);
            }
            System.out.print("\n");
           
        }
    } 

   
  public static void main(String args[]) {
     int n =5;
    patternA m3=new patternA();
    m3.printSquare(n);
    
    
    
  }
}
